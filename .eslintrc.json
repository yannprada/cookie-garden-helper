{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "script"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-param-reassign": "off",
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "no-undef": "off",
    "no-unused-vars": "off",
    "prettier/prettier": "error"
  }
}
